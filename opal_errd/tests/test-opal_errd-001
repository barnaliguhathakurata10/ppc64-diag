#!/bin/bash

#WARNING: DO NOT RUN THIS FILE DIRECTLY
#  This file expects to be a part of ppc64-diag test suite
#  Run this file with ../run_tests -t test-opal_errd-001 -q

check_suite
copy_sysfs

./$OPAL_ERRD_DIR/opal_errd -s $SYSFS -o $OUT/platform -D -e /not/a/valid/path > /dev/null 2>&1
RC=$?
if [ "$RC" -ne 0 ] ; then
	register_fail $RC;
fi

register_success
