#! /bin/bash

#WARNING: DO NOT RUN THIS FILE DIRECTLY
#  This file expects to be a part of ppc64-diag test suite
#  Run this file with ../run_tests -t test-opal-elog-parse-002 -q

check_suite
copy_sysfs

$VALGRIND ./$OPAL_ERRD_DIR/opal-elog-parse/opal-elog-parse -d 2 -f $SYSFS/firmware/opal/elog/0x02/raw &>> /dev/null 1>&2
R=$?
if [ $R -ne 234 ]; then
	register_fail $R;
fi

register_success
